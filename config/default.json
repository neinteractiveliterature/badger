{
    "app": {
        "name": "Badger",
        "debug": false,
        "defaultFont": "Helvetica",
        "print": true,
        "outputDir": "./output",
        "dbURL": "postgres://badger:badger@localhost:5432/badger",
        "dbSSL": false,
        "dbPoolSize": 1,
        "sessionType": "local",
        "sessionSecret": "badger",
        "defaultEventId": 1,
        "printOptions":{
            "copies": 1,
            "landscape": false
        }
    },
    "pages": {
        "ppi": 72,
        "default": "cr80",
        "sizes": {
            "cr80": {
                "height": 2.125,
                "width": 3.375
            }
        }
    }
}
